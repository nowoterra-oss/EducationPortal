GET {{baseUrl}}/api/schedules?pageNumber=1&pageSize=10

### Get Schedule By ID
GET {{baseUrl}}/api/schedules/1

### Create Schedule
POST {{baseUrl}}/api/schedules
Content-Type: application/json

{
  "courseId": 1,
  "dayOfWeek": "Monday",
  "startTime": "09:00:00",
  "endTime": "10:30:00",
  "classroom": "A101"
}

### Update Schedule
PUT {{baseUrl}}/api/schedules/1
Content-Type: application/json

{
  "id": 1,
  "startTime": "10:00:00",
  "endTime": "11:30:00",
  "classroom": "B202"
}

### Delete Schedule
DELETE {{baseUrl}}/api/schedules/1

###############################################################################
# TEACHERS ENDPOINTS
###############################################################################

### Get All Teachers
GET {{baseUrl}}/api/teachers?pageNumber=1&pageSize=10

### Get Teacher By ID
GET {{baseUrl}}/api/teachers/1

### Search Teachers
GET {{baseUrl}}/api/teachers/search?term=ahmet&pageNumber=1&pageSize=10

### Get Teacher Courses
GET {{baseUrl}}/api/teachers/1/courses

### Create Teacher
POST {{baseUrl}}/api/teachers
Content-Type: application/json

{
  "userId": "user-guid-here",
  "subject": "Matematik",
  "yearsOfExperience": 10,
  "qualifications": "PhD Matematik"
}

### Update Teacher
PUT {{baseUrl}}/api/teachers/1
Content-Type: application/json

{
  "id": 1,
  "subject": "İleri Matematik",
  "yearsOfExperience": 11
}

### Delete Teacher
DELETE {{baseUrl}}/api/teachers/1

###############################################################################
# UNIVERSITY APPLICATIONS ENDPOINTS
###############################################################################

### Get All University Applications
GET {{baseUrl}}/api/university-applications?pageNumber=1&pageSize=10

### Get Application By ID
GET {{baseUrl}}/api/university-applications/1

### Get Student Applications
GET {{baseUrl}}/api/university-applications/student/1

### Get Applications By Status
GET {{baseUrl}}/api/university-applications/status/Pending?pageNumber=1&pageSize=10

### Get Application Timeline
GET {{baseUrl}}/api/university-applications/1/timeline

### Get Application Statistics
GET {{baseUrl}}/api/university-applications/statistics

### Create University Application
POST {{baseUrl}}/api/university-applications
Content-Type: application/json

{
  "studentId": 1,
  "universityName": "Harvard University",
  "program": "Computer Science",
  "country": "USA",
  "applicationDate": "2024-11-15T00:00:00Z",
  "deadline": "2025-01-01T00:00:00Z",
  "status": "Pending",
  "applicationFee": 75.00
}

### Update Application Status
PATCH {{baseUrl}}/api/university-applications/1/status
Content-Type: application/json

{
  "status": "Accepted",
  "notes": "Scholarship awarded"
}

### Add Documents to Application
POST {{baseUrl}}/api/university-applications/1/documents
Content-Type: application/json

{
  "documentType": "Transcript",
  "documentUrl": "https://example.com/docs/transcript.pdf"
}

### Update Application
PUT {{baseUrl}}/api/university-applications/1
Content-Type: application/json

{
  "id": 1,
  "status": "Accepted",
  "scholarshipAmount": 10000.00
}

### Delete Application
DELETE {{baseUrl}}/api/university-applications/1

